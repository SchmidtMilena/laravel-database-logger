{
    "name": "schmidtmilena/laravel8-database-logger",
    "description": "Database logger for Laravel 8",
    "keywords": [
        "schmidtmilena",
        "laravel8-database-logger"
    ],
    "homepage": "https://github.com/schmidtmilena/laravel8-database-logger",
    "license": "MIT",
    "authors": [{
        "name": "Milena Schmidt",
        "email": "milenaschmidt@protonmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.1",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "SchmidtMilena\\DbLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SchmidtMilena\\DbLogger\\Tests\\": "tests",
            "SchmidtMilena\\DbLogger\\Database\\Factories\\": "database/factories"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SchmidtMilena\\DbLogger\\Providers\\DbLoggerServiceProvider"
            ],
            "aliases": {
                "DbHandler": "SchmidtMilena\\DbLogger\\DbLoggerHandler",
                "DbFormatter": "SchmidtMilena\\DbLogger\\Formatter",
                "Monolog": "SchmidtMilena\\DbLogger\\Monolog"
            }
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}